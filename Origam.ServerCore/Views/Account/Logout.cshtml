@model Origam.ServerCore.IdentityServerGui.Account.LogoutViewModel


<div class="detailview-container">
    <div class="detailview-module with-title">
        <form asp-action="Logout">
            <input type="hidden" name="logoutId" value="@Model.LogoutId"  />
            <h2>Would you like to logout?</h2>
            <div class="input-form">
                <div class="input-row">
                    <button class="btn btn-primary">Yes</button>
                </div>

            </div>
        </form>
    </div>
</div>