<!DOCTYPE html>
<html>
    <head>
        <title>@System.Configuration.ConfigurationManager.AppSettings.Get("portalTitle")</title>
        <link rel="icon" href="assets/favicon.png"/>
        <link href="css/origam.css" rel="stylesheet" type="text/css" />
        <script src="js/jquery-1.9.1.min.js" type="text/javascript"></script>
        <script src="js/jquery.validate.min.js" type="text/javascript"></script>
        <script src="js/jquery.validate.unobtrusive.min.js" type="text/javascript"></script>
        @RenderSection("head", required: false)
    </head>
    <body>
        <div id="container_logo" class="clearfix wrapper">
            <a href="@ConfigUtils.AboutUrl()" target="_blank">
                <img src="assets/logo_hosting.png" alt="Logo" width="210" />
            </a>
        </div>
        <div id="container_content" class="clearfix wrapper">
            @RenderBody()
        </div><!-- /container_content -->
        <div id="container_footer" class="clearfix wrapper">
            <div id="footer_logo">
                <a href="@ConfigUtils.BottomLogoUrl()" target="_blank">
                    <img src="assets/logo_hosting_bottom.png">
                </a>
            </div>
        </div>
        <script type="text/javascript" src="analytics.js" />
    </body>
</html>