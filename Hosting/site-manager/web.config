<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>
  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
      <file value="logs/logfile.txt" />
      <appendToFile value="true" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger [%ndc] &lt;%property{auth}&gt; - %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="NONE" />
      <appender-ref ref="LogFileAppender" />
    </root>
  </log4net>
	<appSettings>
		<add key="origamvdm_site" value="Hosting" />
		<add key="origamvdm_pool" value="Classic .NET AppPool" />
		<add key="origamvdm_physical_path" value="C:\inetpub\companyFolder" />
		<add key="origamvdm_config_path" value="C:\inetpub\site-manager\hosting.config" />
	</appSettings>
</configuration>
